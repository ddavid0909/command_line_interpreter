command -opt -opt input >input.txt <<appendOutput.txt <output.txt "argument"
Command, Option, Option, NonQuoted, Input, Append Output, Output, Quoted

command -opt input | command <<appendOutput.txt <output.txt "argument"
Command, Option, NonQuoted, Pipeline, Command, Append Output, Output, Quoted

command -opt -opt input | command | command
Command, Option, Option, NonQuoted, Pipeline, Command, Pipeline, Command

command -opt |command
Command, Option, Pipeline, Command

command   input    >input.txt | command <output.txt
Command, NonQuoted, Input, Pipeline, Command, Output

command -opt input -opt >input.txt | command | command | command
Command, Option, NonQuoted, Option, Input, Pipeline, Command, Pipeline, Command, Pipeline, Command

command command
Command, NonQuoted

command -opt | command -opt | command | command -opt input
Command, Option, Pipeline, Command, Option, Pipeline, Command, Pipeline, Command, Option, NonQuoted
